src = $(wildcard *.md)

all:
	javac Frontend/Main.java && javac Backend/Main.java

clean:
	find  . -name  '*.class' -type  f -print -exec  rm -rf  {} \;

runfrontend:
	java Frontend.Main

runbackend:
	java -cp sqlite.jar: Backend.Main

build:
	find  . -name  '*.class' -type  f -print -exec  rm -rf  {} \; && javac Frontend/Main.java && javac -cp sqlite.jar: Backend/Main.java

log_backend:
	cat Backend/Log/backend.log

log_frontend:
	cat Frontend/Log/frontend.log

clean_log:
	rm Backend/Log/backend.log Frontend/Log/frontend.log